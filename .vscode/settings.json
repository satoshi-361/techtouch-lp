{
  "eslint.packageManager": "yarn",
  "eslint.autoFixOnSave": true,
  "eslint.options": {
    "extensions": [".js", ".vue", "ts"]
  },
  "eslint.validate": [
    "javascript",
    {
      "autoFix": true,
      "language": "vue"
    },
    {
      "autoFix": true,
      "language": "typescript"
    }
  ],
  "javascript.implicitProjectConfig.experimentalDecorators": false,
  "eslint.enable": false
}
